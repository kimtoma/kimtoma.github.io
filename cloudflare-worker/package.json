{
  "name": "chat-kimtoma-worker",
  "version": "1.0.0",
  "description": "Cloudflare Worker for chat.kimtoma.com with D1 logging",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "tail": "wrangler tail",
    "db:create": "wrangler d1 create chat-logs-db",
    "db:init": "wrangler d1 execute chat-logs-db --file=./src/schema.sql",
    "db:query": "wrangler d1 execute chat-logs-db --command"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "d1",
    "chat",
    "gemini"
  ],
  "author": "kimtoma",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250106.0",
    "wrangler": "^4.61.0"
  }
}
